gzip on;
gzip_comp_level 5;
gzip_min_length 256;
gzip_proxied any;
gzip_vary on;
gzip_types
	application/atom+xml
	application/javascript
	application/json
	application/octet-stream
	application/rss+xml
	application/vnd.ms-fontobject
	application/x-javascript
	application/xml
	font/opentype
	font/truetype
	image/svg+xml
	text/css
	text/html
	text/javascript
	text/plain
	text/x-component
	text/xml;